<% if @profile.supported? %>
  <%= t(".supported", zipcode: @profile.zipcode) %>
<% else %>
  <%= t(".unsupported", zipcode: @profile.zipcode) %>
<% end %>

<%= render("locations/form", location: @profile.location || @profile.build_location) %>

<% if @profile.location.persisted? %>
  <%= t(".pickup") %>
<% end %>
